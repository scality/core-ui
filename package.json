{
  "name": "@scality/core-ui",
  "version": "0.10.2",
  "description": "Scality common React component library",
  "author": "Scality Engineering",
  "license": "SEE LICENSE IN LICENSE",
  "main": "dist/index.js",
  "mainSrc": "src/lib/index.js",
  "scripts": {
    "analyze": "source-map-explorer 'dist/*.js'",
    "build": "rimraf dist && cross-env NODE_ENV=production babel src/lib --out-dir dist --copy-files && rimraf dist/**/*.test.js && rimraf dist/**/__snapshots__",
    "flow": "flow check",
    "lint": "eslint src/* --ext .js --no-error-on-unmatched-pattern",
    "prepare": "npm run build",
    "plop": "plop",
    "test": "jest",
    "storybook": "start-storybook -p 3000",
    "storybook:deploy": "storybook-to-ghpages"
  },
  "keywords": [],
  "devDependencies": {
    "@babel/cli": "7.6.0",
    "@babel/core": "7.6.0",
    "@babel/preset-env": "7.6.0",
    "@babel/preset-flow": "7.0.0",
    "@babel/preset-react": "7.0.0",
    "@fortawesome/fontawesome-free": "^5.10.2",
    "@fortawesome/fontawesome-svg-core": "^1.2.35",
    "@fortawesome/free-regular-svg-icons": "^5.15.3",
    "@fortawesome/free-solid-svg-icons": "^5.15.3",
    "@fortawesome/react-fontawesome": "^0.1.14",
    "@storybook/addon-actions": "^6.1.20",
    "@storybook/addon-essentials": "^6.1.20",
    "@storybook/addon-knobs": "^6.1.20",
    "@storybook/addon-links": "^6.1.20",
    "@storybook/addon-storyshots": "^6.1.20",
    "@storybook/addon-storysource": "^6.1.21",
    "@storybook/react": "^6.1.20",
    "@storybook/storybook-deployer": "^2.8.7",
    "@storybook/theming": "^6.1.20",
    "@typescript-eslint/eslint-plugin": "^2.3.0",
    "@typescript-eslint/parser": "^2.3.0",
    "babel-eslint": "^10.0.3",
    "babel-loader": "8.0.6",
    "cross-env": "^7.0.3",
    "eslint": "^7.20.0",
    "eslint-config-react-app": "^5.0.2",
    "eslint-plugin-flowtype": "^3.13.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.14.3",
    "eslint-plugin-react-hooks": "^1.7.0",
    "flow-bin": "^0.107.0",
    "husky": "^3.0.5",
    "jest": "^24.9.0",
    "jest-canvas-mock": "^2.3.1",
    "plop": "^2.4.0",
    "polished": "^3.4.1",
    "react": "^16.9.0",
    "react-debounce-input": "3.2.2",
    "react-dom": "^16.9.0",
    "react-select": "3.0.3",
    "react-storybook-addon-props-combinations": "^1.1.0",
    "react-test-renderer": "^16.9.0",
    "react-virtualized": "^9.21.1",
    "rimraf": "^3.0.0",
    "source-map-explorer": "^2.0.1",
    "storybook-addon-designs": "^6.1.0",
    "styled-components": "^5.2.1",
    "vega": "^5.17.3",
    "vega-embed": "^6.0.0",
    "vega-lite": "^5.0.0"
  },
  "peerDependencies": {
    "@fortawesome/fontawesome-free": "5.7.2",
    "polished": "3.4.1",
    "react": "^16.8.0",
    "react-dom": "^16.8.0",
    "react-debounce-input": "3.2.2",
    "react-virtualized": "9.21.0",
    "react-select": "3.0.3",
    "styled-components": "^4.1.2",
    "vega": "^5.17.3",
    "vega-embed": "^6.0.0",
    "vega-lite": "^5.0.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint"
    }
  },
  "jest": {
    "setupFiles": [
      "jest-canvas-mock"
    ],
    "transform": {
      "^.+\\.[tj]sx?$": "babel-jest",
      "^.+\\.mdx?$": "@storybook/addon-docs/jest-transform-mdx"
    }
  }
}
